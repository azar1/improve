<script>

(function () {
    $(function () {
        var $accordion, $wideScreen;
        $accordion = $('#puNextLink-02').children('.puNLSlides');
        $wideScreen = $(window).width() > 1;
        if ($wideScreen) {
            $accordion.on('click', function (e) {
                var $this;
                e.stopPropagation();
                $this = $(this);
				$this.children('h1').css({'transform':'rotate(0deg)'});
                if ($this.hasClass('out')) {
                    $this.addClass('out');
                } else {
                    $this.addClass('out');
                    $this.siblings().removeClass('out');
                }
            });
        } else {
            $accordion.on('touchstart touchend', function (e) {
                var $this;
                e.stopPropagation();
                $this = $(this);
                if ($this.hasClass('out')) {
                    $this.addClass('out');
                } else {
                    $this.addClass('out');
                    $this.siblings().removeClass('out');
                }
            });
        }
    });

}.call(this));

</script>

